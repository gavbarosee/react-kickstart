{
  "name": "react-kickstart",
  "version": "0.1.0",
  "type": "module",
  "description": "A modern CLI tool for creating React applications with various frameworks",
  "main": "src/index.js",
  "bin": {
    "react-kickstart": "bin/react-kickstart.js"
  },
  "scripts": {
    "start": "node bin/react-kickstart.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --silent",
    "lint": "eslint src bin --ext .js,.cjs",
    "lint:fix": "eslint src bin --ext .js,.cjs --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "qa:critical": "node qa-automation/test-runner.js critical",
    "qa:standard": "node qa-automation/test-runner.js standard",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "cli",
    "generator",
    "vite",
    "nextjs"
  ],
  "author": "Gav Barosee",
  "license": "MIT",
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "cross-spawn": "^7.0.3",
    "execa": "^9.6.0",
    "figlet": "^1.7.0",
    "figures": "^6.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.9.3",
    "log-symbols": "^7.0.1",
    "open": "^10.1.0",
    "ora": "^8.2.0",
    "validate-npm-package-name": "^6.0.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "globals": "^15.15.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "prettier": "^3.6.2",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "bin/**/*.{js,cjs}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16"
  }
}
